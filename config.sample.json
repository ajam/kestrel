{
	"github_listener": {
		"account_name": "your-github-account-name",
		"port": 9001
	},
	"s3": {
		"enabled": false,
		"bucket_name": "s3-bucket-name",
		"path": "2014/",
		"sync_deploy_trigger": "string-to-trigger-a-sync-to-s3",
		"exclude_from_sync": [".git/*", ".*"],
		"hard_deploy": {
			"enabled": false,
			"trigger": "string-to-trigger-an-overwrite-of-files-on-s3"
		}
	},
	"archive": {
		"enabled": false,
		"account_name": "your-github-or-bitbucket-account-name",
		"type": "choose-bitbucket-or-github"
	},
	"verify_committer": {
		"enabled": false,
		"team_id": "000001",
		"access_token": "your-access-token"
	}
}
